<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<addon id="plugin.audio.musicip" name="MusicIP Mixer" provider-name="Pernod" version="0.0.1">
  <requires>
    <import addon="xbmc.python" version="2.1.0" />
    <import addon="script.module.simplejson" version="2.0.10" />
  </requires>
  <extension library="default.py" point="xbmc.python.pluginsource">
        <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">MusicIP plugin</summary>
    <description lang="en">Plugin that allows interaction with your MusicIP server. MusicIP analyses your music collection and generates an &quot;acoustic fingerprint&quot; of each song. This information is then used to generate a personal playlist of similar tracks based on a selected track, artist or album.</description>
    <platform>all</platform>
    <website>www.xbmc.org</website>
    <source>https://github.com/pernod/plugin.audio.musicip</source>
    <email>pernod@xbmc.org</email>
    <forum>http://forum.xbmc.org/showthread.php?tid=112150</forum>
    <license>GPL v2.0</license>
  </extension>
</addon>